{
    "appearance": {
        "font_family": "Hack Nerd Font",
        "font_size": 14,
        "font_color": "#49a10f",
        "opacity": 100,
        "text_align": "left"
    },
    "position": {
        "monitor": 1,
        "anchor": "bottom_left",
        "offset_x": 5,
        "offset_y": 50,
        "width": 360,
        "auto_width": true
    },
    "visualization": {
