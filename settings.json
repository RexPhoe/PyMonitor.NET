            ],
            "Conexi\u00f3n de \u00e1rea local* 10": [],
            "Conexi\u00f3n de \u00e1rea local* 9": [],
            "Conexi\u00f3n de red Bluetooth": [],
            "Ethernet": [
                "Data Uploaded",
                "Data Downloaded",
                "Network Utilization"
            ],
            "Wi-Fi": []
        },
        "component_order": [],
        "show_component_titles": false,
        "sensor_indentation": 0,
        "category_spacing": 1,
        "display_mode": "multiline",
        "show_icons": true
    },
    "monitoring": {
        "update_interval": 2,
        "temperature_unit": "celsius",
        "enabled_hardware": [
